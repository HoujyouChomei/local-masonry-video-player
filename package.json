{
  "name": "local-masonry-video-player",
  "productName": "Local Masonry Video Player",
  "version": "0.2.6",
  "description": "A local video player designed for browsing and managing collections of AI-generated content and video assets, featuring a dynamic masonry grid layout.",
  "private": true,
  "author": "HoujyouChomei",
  "license": "MIT",
  "homepage": "https://github.com/HoujyouChomei/local-masonry-video-player",
  "repository": {
    "type": "git",
    "url": "https://github.com/HoujyouChomei/local-masonry-video-player.git"
  },
  "keywords": [
    "electron",
    "video-player",
    "masonry-layout",
    "ai-content",
    "local-management"
  ],
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "concurrently -k \"node build-electron.mjs --dev\" \"wait-on dist-electron/main.js && electron .\"",
    "build": "npm run licenses && tsc -b && vite build && node build-electron.mjs",
    "preview": "vite preview",
    "lint": "eslint .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "test": "vitest",
    "licenses": "license-checker-rseidelsohn --json --out public/licenses.json --production --unknown --excludePrivatePackages",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.11",
    "better-sqlite3": "^12.5.0",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "electron-log": "^5.4.3",
    "electron-store": "^8.1.0",
    "electron-window-state": "^5.0.3",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "qrcode.react": "^4.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^10.0.0",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^7.1.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.2.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/chokidar": "^1.7.5",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.1.1",
    "electron-builder": "^26.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "lint-staged": "^15.4.3",
    "playwright": "^1.57.0",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.6",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.example.local-masonry-video-player",
    "productName": "Local Masonry Video Player",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "dir",
        "zip"
      ]
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
